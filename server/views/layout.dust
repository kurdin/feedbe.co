<script>
$().ready(function () {
  $('.nav-toggle').on('click', function (ev) {
    ev.preventDefault();
    let els = [this];
    els.push($(this).parent().find('.nav-men'));
    $(els).toggleClass('is-active').parent().find('.nav-menu').toggleClass('is-active');
  });

  $(window).resize(function() {
    $('#top').find('.is-active').toggleClass('is-active');
  });
});
</script>

